{
	// "extension-test-runner.debugOptions": {},
	"git.branchProtection": [
		"main"
	],
	"eslint.options": {
		"exclude": [
			"**/dist/**"
		]
	},
	"files.associations": {
		"ablunit-test-profile*.json": "jsonc"
	},
	"files.exclude": {
		"**/*.js": { "when": "$(basename).ts" }
	},
	"files.watcherExclude": {
		".git/objects": true,
		".git/subtree-cache": true,
		".vscode-test": true,
		"coverage": true,
		"dist": true,
		"out": true,
		"node_modules": true
	},
	"json.schemas": [
		{
			"fileMatch": [
				"ablunit-test-profile*.json",
				"ablunit-test-profile*.jsonc",
			],
			"url": "./resources/ablunit-test-profile.schema.json"
		},
		{
			"fileMatch": [
				"webpack.config.json",
			],
			"url": "https://raw.githubusercontent.com/webpack/webpack/main/schemas/WebpackOptions.json"
		}
	],
	"npm.autoDetect": "on",
	"redhat.telemetry.enabled": false,
	"search.exclude": {
		".vscode-test": true,
		"coverage": true,
		"dist": true,
		"out": true,
		"node_modules": true,
	},
	"shellcheck.useWorkspaceRootAsCwd": true,
	"sonarlint.connectedMode.project": {
		"connectionId": "sonarcloud-kenherring",
		"projectKey": "kenherring_ablunit-test-runner"
	},
	"sonarlint.disableTelemetry": true,
	"task.autoDetect": "on",
	"typescript.preferences.quoteStyle": "single",
	"typescript.validate.enable": true,
}
